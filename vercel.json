{
    "version":2,
    "name":"food-app",
    "builds":[
        {
            "src":"./index.js","use": "@vercel/node"}
        
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/routes/index.js"
        },
        {
          "src": "/createuser",
          "dest": "/routes/Createdata.js"
        },
        {
            "src":"/loginuser",
            "dest":"/routes/Createdata.js"
        },
      ]
    }
}